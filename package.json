{
  "name": "@playding/redis-simple-csc",
  "version": "0.2.0",
  "description": "A minimal ~80-line client-side cache implementation for Redis with RESP3 protocol support",
  "main": "src/simple-cache.js",
  "type": "commonjs",
  "exports": {
    ".": "./src/simple-cache.js"
  },
  "files": [
    "src/simple-cache.js",
    "README.md",
    "LICENSE",
    "docs/SIMPLE-CACHE.md",
    "docs/USAGE.md"
  ],
  "scripts": {
    "test": "node --test test/*.js",
    "test:unit": "node --test test/test-simple-cache.js",
    "test:complex": "node --test test/test-complex-scenarios.js",
    "test:coverage": "node --test --experimental-test-coverage test/*.js",
    "demo": "node examples/demo.js",
    "worker": "node examples/worker.js",
    "master": "node examples/master.js",
    "master:auto": "node examples/master.js auto",
    "prepublishOnly": "npm test",
    "version": "npm run test && git add -A",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yidinghan/redis-client-side-simple-cache.git"
  },
  "bugs": {
    "url": "https://github.com/yidinghan/redis-client-side-simple-cache/issues"
  },
  "homepage": "https://github.com/yidinghan/redis-client-side-simple-cache#readme",
  "keywords": [
    "redis",
    "cache",
    "client-side-cache",
    "client-side-caching",
    "resp3",
    "invalidation",
    "tracking",
    "node-redis",
    "performance",
    "minimal"
  ],
  "author": "playdingnow <playdingnow@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "redis": "^4.0.0 || ^5.0.0"
  },
  "peerDependencies": {
    "redis": "^4.0.0 || ^5.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
